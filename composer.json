{
    "name": "luis/wetek-challenge",
    "description": "An app for importing EPG files data into different databases",
    "keywords": ["microframework", "rest", "router", "psr7", "cli"],
    "homepage": "https://github.com/adrianfalleiro/slim-cli-runner",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Felipe Morcillo",
            "email": "lfblast@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.12",
        "adrianfalleiro/slim-cli-runner": "^2",
        "faapz/pdo": "^1.11",
        "mongodb/mongodb": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "WetekChallenge\\": "src/",
            "WetekChallenge\\Repo\\": "src/Repository/",
            "WetekChallenge\\Service\\": "src/Service/",
            "WetekChallenge\\Controller\\Cli\\": "src/Controller/Cli"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "cli": "php public/index.php"
    }
}
